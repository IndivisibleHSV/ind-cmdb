{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "script"
  },
  "rules": {
    "indent": [2, 4],
    "consistent-return": [0],
    "id-length": [0],
    "no-param-reassign": [0],
    "no-shadow": [0],
    "spaced-comment": [0],
    "comma-dangle": [0],
    "func-names": [0],
    "padded-blocks": [0],
    "arrow-body-style": [0],
    "max-len": [0],
    "newline-per-chained-call": [0],
    "arrow-parens": [2, "as-needed"],
    "lines-around-directive": [0],
    "import/newline-after-import": [0]
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base"
}
